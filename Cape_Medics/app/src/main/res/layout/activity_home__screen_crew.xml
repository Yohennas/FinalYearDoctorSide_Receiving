<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home_Screen">

    <Button
        android:id="@+id/DashBoard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="Dash"
        android:text="Dash Board"
        tools:layout_editor_absoluteX="63dp"
        tools:layout_editor_absoluteY="115dp" />

    <Button
        android:id="@+id/CheckIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="CheckIn"
        android:text="Check-In"
        tools:layout_editor_absoluteX="246dp"
        tools:layout_editor_absoluteY="114dp" />

    <Button
        android:id="@+id/HR_Forms"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="HR"
        android:text="HR Forms"
        tools:layout_editor_absoluteX="62dp"
        tools:layout_editor_absoluteY="397dp" />

    <Button
        android:id="@+id/MedicalRecords"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="MedRec"
        android:text="Medical Records"
        tools:layout_editor_absoluteX="226dp"
        tools:layout_editor_absoluteY="397dp" />

    <Button
        android:id="@+id/TimeSheet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="TimeSheet"
        android:text="Time Sheets"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="564dp" />
</android.support.constraint.ConstraintLayout>